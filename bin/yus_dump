#!/usr/bin/env ruby

$: << File.expand_path('../lib', File.dirname(__FILE__))

require 'rubygems' if RUBY_VERSION.match('1.8.6')
require 'drb'
require 'drb/ssl'
require 'password'
require 'rclconf'
require 'yus/session'
require 'yus/helpers'
require 'getoptlong'

opts = YUS.default_opts
filename = ARGV.first

session = YUS.session(opts)
session.dump_to_yaml(filename)

